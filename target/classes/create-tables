CREATE TABLE `sys`.`enrollee` (
  `enrollee_id` INT NOT NULL AUTO_INCREMENT,
  `enrollee_first_name` VARCHAR(45) NOT NULL,
  `enrollee_last_name` VARCHAR(45) NOT NULL,
  `enrollee_activated` TINYINT NOT NULL,
  `enrollee_birth_date` DATETIME NOT NULL,
  `enrollee_telephone` INT NULL,
  PRIMARY KEY (`enrollee_id`));

  CREATE TABLE `sys`.`dependents` (
    `dependent_id` INT NOT NULL AUTO_INCREMENT,
    `enrolleee_id` INT NOT NULL,
    `dependent_first_name` VARCHAR(45) NOT NULL,
    `dependent_last_name` VARCHAR(45) NOT NULL,
    `dependent_birth_date` DATETIME NOT NULL,
    PRIMARY KEY (`dependent_id`));


